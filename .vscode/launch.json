    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch without path",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal"
        }
        {
            "name": "Launch with invalid path",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["fakefile.txt"]
        }
        {
            "name": "Launch with valid path",
            "type": "debugpy",
            "request": "launch",
            "program": "${file}",
            "console": "integratedTerminal",
            "args": ["gateway.log"]
        
        }
    ]
}